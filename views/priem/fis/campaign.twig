<Root>
    <AuthData>
        <Login>{{ fis_login }}</Login>
        <Pass>{{ fis_pass }}</Pass>
    </AuthData>
    <PackageData>
        <CampaignInfo>
            <Campaigns>
                <Campaign>
                    <UID>{{ Code }}</UID>
                    <Name>{{ Description }}</Name>
                    <YearStart>{{ ГодНачала }}</YearStart>
                    <YearEnd>{{ ГодОкончания }}</YearEnd>
                    <EducationForms>
                        <EducationFormID>11</EducationFormID>
                    </EducationForms>
                    <StatusID>1</StatusID>
                    <EducationLevels>
                        <EducationLevelID>17</EducationLevelID>
                    </EducationLevels>
                    <CampaignTypeID>3</CampaignTypeID>
                </Campaign>
            </Campaigns>
        </CampaignInfo>
        <AdmissionInfo>
            <AdmissionVolume>
                {% for item in admission %}
                <Item>
                    <UID>{{ ГодНачала }}_{{ item.id }}</UID>
                    <CampaignUID>000000001</CampaignUID>
                    <EducationLevelID>17</EducationLevelID>
                    <DirectionID>{{ item.КодЭлемента }}</DirectionID>
                    <NumberBudgetO>{{ item.План }}</NumberBudgetO>
                    <NumberBudgetOZ>0</NumberBudgetOZ>
                    <NumberBudgetZ>0</NumberBudgetZ>
                    <NumberPaidO>0</NumberPaidO>
                    <NumberPaidOZ>0</NumberPaidOZ>
                    <NumberPaidZ>0</NumberPaidZ>
                    <NumberTargetO>0</NumberTargetO>
                    <NumberTargetOZ>0</NumberTargetOZ>
                    <NumberTargetZ>0</NumberTargetZ>
                    <NumberQuotaO>0</NumberQuotaO>
                    <NumberQuotaOZ>0</NumberQuotaOZ>
                    <NumberQuotaZ>0</NumberQuotaZ>
                    <IsPlan>true</IsPlan>
                    <IsUGS>false</IsUGS>
                </Item>
                {% endfor %}
            </AdmissionVolume>
            <CompetitiveGroups>
                {% for item in admission %}
                <CompetitiveGroup>
                    <UID>{{ ГодНачала }}_{{ item.id }}</UID>
                    <CampaignUID>{{ Code }}</CampaignUID>
                    <Name>{{ item.ПрефиксУчебныхГрупп }}</Name>
                    <EducationLevelID>17</EducationLevelID>
                    <EducationSourceID>14</EducationSourceID>
                    <EducationFormID>11</EducationFormID>
                    <DirectionID>{{ item.КодЭлемента }}</DirectionID>
                    <LevelBudget>2</LevelBudget>
                    <StudyPeriod>34</StudyPeriod>
                    <StudyBeginningDate>{{ ГодНачала }}-09-01T00:00:00</StudyBeginningDate>
                    <StudyEndingDate>{{ ГодНачала + 3 }}-06-30T00:00:00</StudyEndingDate>
                    <CompetitiveGroupItem>
                        <NumberBudgetO>{{ item.План }}</NumberBudgetO>
                    </CompetitiveGroupItem>
                </CompetitiveGroup>
                {% endfor %}
            </CompetitiveGroups>
        </AdmissionInfo>
    </PackageData>
</Root>