  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-ctype": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-fileinfo": "*",
    "ext-dom": "*",
    "phpfastcache/phpfastcache": "^9.1",
    "filp/whoops": "^2.14",
    "dompdf/dompdf": "^1.2",
    "twig/twig": "^3.0",
    "ext-curl": "*",
    "kilylabs/odata-1c": "^1.2",
    "gabordemooij/redbean": "^5.7"
  }
}
